<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">
    <changeSet id="2020-01-19-fd-user" author="ztxemail@163.com">
       <createTable tableName="fd_user">
           <column name="id" type="BIGINT UNSIGNED" autoIncrement="true" remarks="主键">
               <constraints primaryKey="true"></constraints>
           </column>
           <column name="login_name" type="VARCHAR(255)" remarks="登录名">
               <constraints nullable="false"/>
           </column>
           <column name="password" type="VARCHAR(255)"  remarks="密码">
               <constraints nullable="false"/>
           </column>
           <column name="version_number" type="BIGINT UNSIGNED"  defaultValue="1" remarks="乐观锁"/>
           <column name="created_date" type="DATETIME" defaultValueComputed="CURRENT_TIMESTAMP" remarks="创建时间"/>
       </createTable>
    </changeSet>
</databaseChangeLog>